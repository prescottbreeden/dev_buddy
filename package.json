{
  "name": "date_watcher",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "precommit": "lint-staged",
    "getstart": "yarn & node-sass src/sass/main.scss src/index.css & react-scripts start",
    "test:ci": "react-scripts test --watchAll=false --reporters=default --reporters=jest-junit",
    "coverage": "react-scripts test --coverage --watchAll",
    "watch:sass": "node-sass src/sass/main.scss src/index.css -w",
    "compile:sass": "node-sass src/sass/main.scss src/index.css",
    "prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,json,md}\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/ramda": "^0.27.4",
    "@types/react-redux": "^7.1.7",
    "@types/redux-devtools": "^3.0.47",
    "husky": "^4.2.5",
    "node-sass": "^4.14.0",
    "prettier": "^2.0.5",
    "ramda": "^0.27.0",
    "redux-devtools": "^3.5.0",
    "redux-devtools-inspector": "^0.11.3"
  }
}
